<%= render "shared/header1" %>

<div class= 'team-main'>
  <h1 class= 'main-header'>+Plan</h1>
  <%= form_with model: [@company,@plan], class: 'form', local: true do |f| %>
    <div class="form-input">
      <%= f.text_field :plan_name, class: 'form-company', placeholder: 'type a plan name' , autocomplete: 'off'%>
    </div>
    <%= f.submit '追加', class: 'form-submit' %>
    <%= link_to 'Back', company_path(@company), class: 'btn'%>
  <% end %>
</div>